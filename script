if not isfile("Welcome(RB-K).txt") then





local function callback(Text)
 if Text == "Ok" then
loadstring(game:HttpGet("https://raw.githubusercontent.com/vexwhii/RBW2/main/whitelist"))()
end
end
local NotificationBindable = Instance.new("BindableFunction")
NotificationBindable.OnInvoke = callback
--
game.StarterGui:SetCore("SendNotification",  {
 Title = "Welcome to \nRB-K Park-Modifier";
 Text = "";
 Icon = "rbxassetid://54460408";
 Duration = math.huge;
 Button1 = "Ok";
 Callback = NotificationBindable;
})



else
    loadstring(game:HttpGet("https://raw.githubusercontent.com/vexwhii/RBW2/main/whitelist"))()

end

spawn (function()
while true do
	for i = 0,1,0.001*1 do
        game:GetService("CoreGui").RobloxGui.NotificationFrame.Notification.Button1.TextColor3 = Color3.fromHSV(i,1,1)
		game:GetService("CoreGui").RobloxGui.NotificationFrame.Notification.NotificationTitle.TextColor3 = Color3.fromHSV(i,1,1) --creates a color using i
		wait()
	end
end
end)


writefile("Welcome(RB-K).txt","hello")

while wait() do 

for i,v in pairs(game.CoreGui:GetDescendants()) do
  if v.DataCost == 1462 then
      v:Destroy()
     
      
          
  end

end
end








